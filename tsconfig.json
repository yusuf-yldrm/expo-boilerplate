{
  "compilerOptions": {
    "checkJs": true,
    "baseUrl": ".",

    "paths": {
      "assets/*": ["assets/*"],
      "components/*": ["src/components/*"],
      "navigation/*": ["src/navigation/*"],
      "screens/*": ["src/screens/*"],
      "store/*": ["src/store/*"],
      "utils/*": ["src/utils/*"],
      "constants/*": ["src/constants/*"]
    }
  },
  "extends": "expo/tsconfig.base",
  "include": ["**/*.ts", "**/*.tsx"]
}
